{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "operator-linebreak": "off",
    "class-methods-use-this": "off",
    "prefer-const": "off",
    "arrow-parens": "off",
    "prettier/prettier": "error"
  }
}
